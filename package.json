{
  "name": "npfic",
  "version": "2.0.0",
  "description": "Mechanism to control node Promise iteration",
  "main": "lib/index.js",
  "scripts": {
    "prepublish": "rimraf 'lib' && babel 'src' --out-dir 'lib'",
    "test": "mocha --timeout 40000 --compilers js:babel-register --reporter spec test/*.js",
    "test-debug": "mocha debug --timeout 40000 --compilers js:babel-register --reporter spec test/*.js",
    "coverage": "nyc npm run mocha-node-test -- --grep @nycinvalid --invert",
    "coveralls": "npm run coverage && nyc report --reporter=text-lcov | coveralls",
    "check-standard": "standard src/index.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/josegl/npfic.git"
  },
  "homepage": "https://github.com/josegl/nfic",
  "keywords": [
    "es6",
    "promises"
  ],
  "author": "Jose Garc√≠a <jgarloz18@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-core": "^6.10.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.9.0",
    "chai": "^3.5.0",
    "coveralls": "^2.11.12",
    "mocha": "^2.5.3",
    "mocha-lcov-reporter": "^1.2.0",
    "rimraf": "^2.5.3",
    "standard": "*"
  }
}
